        <require>
            <i>STAT caster MP  >= 2</i>
        </require>
        <group>
            <i>damage</i>
            <i>active</i>
            <i>globe</i>
        </group>
    </fire>
    
    <water>
        <DESC>Waterfall</DESC>
        <effect>
            <i>CHANGE target HP -((S C MGK + DICE 10) * (1 - S T MDEF / 100) * (S T WATER / 2))</i>
