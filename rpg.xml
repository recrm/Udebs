<udebs>

<config>
    <name>Sub Battle</name>
</config>

<definitions>   
    <stats>
        <ACT /> <!-- Number of action points -->
        <SPD /> <!-- How many action points are gained each tick -->
        <HP />  <!-- Hit points -->
        <MP />  <!-- Magic points -->
        <MEL /> <!-- Mellee Damage -->
        <MGK /> <!-- Magic Damage -->
        <FIRE /> <!-- Fire weakness -->
        <WATER /> <!-- Water weakness -->
        <DEF /> <!-- physical defense -->
        <MDEF /> <!-- magical defense -->
        <LVL /> <!-- Recruiter Level -->
        <r />   <!-- Red -->
        <g />   <!-- Green -->
        <b />   <!-- Blue -->
    </stats>

    <strings>
        <DESC /> <!-- Printed Descriptions -->
        <skip />
    </strings>

    <lists>
        <status rlist='' slist='status' /> <!-- Status effects, effects stats -->
        <equip rlist='' slist='equip_item' /> <!-- Equip effects, effects stats -->
        <inventory /> <!-- Place for storing items, party only -->
        <monsters />  <!-- List of spawnable monsters, recruiter only -->
        <yesequip />  <!-- List of people that can equip this item. Equipable items only -->
        <movelist />
    </lists>
</definitions>

<map>
    <row>bmage, empty</row>
    <row>fighter, empty</row> 
    <row>wmage, empty</row>
</map>

<entities>
    <!-- Script-->
    <tick>
        <effect>
            <i>CAST (ALL party) atb</i>
            <i>CAST (ALL monster) atb</i>
        </effect>
    </tick>
    
    <atb>
        <effect>CHANGE T ACT (S T SPD)</effect>
    </atb>

    <init>
        <effect>
            <i>CAST recruiter recruitBattle</i>
            <i>CHANGE fighter ACT (-(STAT fighter ACT)+ 1)</i>
            <i>CHANGE bmage ACT (-(STAT bmage ACT)+ 1)</i>
            <i>CHANGE wmage ACT (-(STAT wmage ACT)+ 1)</i>
        </effect>
    </init>

    <finish>
        <effect>
            <i>CHANGE recruiter LVL 1</i>
            <i>CHANGE treasure LVL 1</i>
        </effect>
    </finish>
    
    <recruit>
        <effect>CHANGE target SPD 1</effect>
    </recruit>
    
    <recruit_boss>
        <effect>
            <i>boss TRAVEL (1,1)</i>
            <i>GETS boss group monster</i>
        </effect>
    </recruit_boss>

    <!-- meta moves -->
    <recruitBattle>
        <effect>
            <i>RECRUIT ((S recruiter monsters)[(DICE 3 + S recruiter LVL * 3)]) (1,0)</i>
            <i>GETS (1,0) group monster</i>
            <i>RECRUIT ((S recruiter monsters)[(DICE 3 + S recruiter LVL * 3)]) (1,1)</i>
            <i>GETS (1,1) group monster</i>
            <i>RECRUIT ((S recruiter monsters)[(DICE 3 + S recruiter LVL * 3)]) (1,2)</i>
            <i>GETS (1,2) group monster</i>
        </effect>
        <require>target.name == 'recruiter'</require>
    </recruitBattle>
    
    <get_item>
        <effect>GETS T inventory ((S C inventory)[(DICE 5 + S C LVL * 6)])</effect>
        <require>caster.name == 'treasure'</require>
    </get_item>
    
    <!-- move groups -->
    <status>
        <DESC>status and buff</DESC>
        <require>
            <i>move.name not in S T status</i>
        </require>
    </status>
    
    <damage>
        <DESC>Damaging moves</DESC>
        <effect>
            <i>DELAY (CAST target KO) 0</i>
        </effect>
    </damage>
    
    <health>
        <DESC>Healing moves</DESC>
        <effect>
            <i>DELAY (CAST target OVERHEAL) 0</i>
        </effect>
    </health>
    
    <regen>
        <DESC>Mana Regen Moves</DESC>
        <effect>
            <i>DELAY (CAST target OVERMANA) 0</i>
       </effect>
    </regen>
    
    <active>
        <DESC>Actives Moves</DESC>
        <require>
            <i>STAT caster ACT >= 2</i>
            <i>move.name in STAT caster movelist</i>
        </require>
        <effect>
            <i>CHANGE caster ACT -2</i>
        </effect>
    </active>
    
    <globe>
        <DESC>Almost all moves are in this class</DESC>
        <require>
            <i>'KO' not in STAT caster status</i>
            <i>'KO' not in STAT target status</i>
        </require>
    </globe>
    
<!-- Status Effects and chaincasts -->
    <KO>
        <DESC>Character's HP has dropped to 0</DESC>
        <require>
            <i>STAT target HP &lt;= 0</i>
        </require>
        <effect>
            <i>CHANGE target HP -(STAT target HP)</i>
            <i>GETS target status move</i>
            <i>LOSES T group monster</i>
        </effect>
        <group>
            <i>globe</i>
            <i>status</i>
        </group>
        <SPD>-1</SPD>
    </KO>
    
    <SACRIFICE>
        <DESC>White Mage has been sacrificed and cannot return to the fight</DESC>
        <group>
            <i>globe</i>
            <i>status</i>
        </group>
        <SPD>-1</SPD> 
    </SACRIFICE>
    
    <DEFEND>
        <DESC>Increased def on fighter, and he guerenteed takes the next hit</DESC>
        <group>
            <i>globe</i>
            <i>status</i>
        </group> 
        <def>50</def>
        <mdef>30</mdef>
    </DEFEND>
    
    <CHARGE>
        <DESC>Decreases fighter def, but charges up for a huge hit</DESC>
        <group>
            <i>globe</i>
            <i>status</i>
        </group>
        <def>-50</def>
        <mdef>-30</mdef>    
    </CHARGE>
 
    <POISON>
        <DESC>POISON</DESC>
        <group>
            <i>status</i>
            <i>globe</i>
        </group>
        <r>-30</r>
        <g>-30</g>
        <b>-30</b>      
    </POISON>
    
<!-- Active components -->
    <POISON_CHECK>
        <DESC>DROPS THE CHARACTERS HEALTH ONCE USING A DELAYED CHAIN MOVE</DESC>
        <require>
            <i>DICE 5 > 2</i>
        </require>
        <effect>
            <i>DELAY (CAST target POISON_ACTIVE) 2</i>
            <i>GETS target status POISON</i>
        </effect>
        <group>
            <i>globe</i>
        </group>
    </POISON_CHECK>
    
    <OVERHEAL>
        <DESC>A check to make sure character hasn't overhealed</DESC>
        <require>
            <i>STAT target HP >= STAT (CLASS target TEMPLATE) HP</i>
            <i>'party' in STAT target group</i>
        </require>
        <effect>
            <i>CHANGE target HP (STAT (CLASS T TEMPLATE) HP - STAT T HP)</i>
        </effect>
    </OVERHEAL>
    
    <OVERMANA>
        <DESC>A check to make sure character hasn't overmanad</DESC>
        <require>
            <i>STAT target MP >= STAT (CLASS target TEMPLATE) MP</i>
            <i>'party' in STAT T group</i>
        </require>
        <effect>
            <i>CHANGE target MP (STAT (CLASS target TEMPLATE ) MP - STAT target MP)</i>
        </effect>
    </OVERMANA>
    
    <POISON_ACTIVE>
        <DESC>Toxic Spit Active Part</DESC>
        <effect>
            <i>CHANGE target HP -(5+ DICE 5)</i>
            <i>DELAY (CAST target POISON_ACTIVE) 2</i>
        </effect>
        <require>
            <i>'POISON' in STAT target status</i>
        </require>
        <group>
            <i>damage</i>
            <i>globe</i>
        </group>
    </POISON_ACTIVE>
    
    <CHARGE_ACTIVE>
        <DESC>Charge Active Component</DESC>
        <effect>
            <i>CHANGE target HP -((S C MEL + DICE 10) * 2 * (1 - S T DEF / 100))</i>
            <i>LOSES caster status CHARGE</i>
        </effect>
        <group>
            <i>damage</i>
            <i>globe</i>
        </group>
    </CHARGE_ACTIVE>

    <DEFEND_ACTIVE>
        <DESC>Defend Active Component</DESC>
        <effect>
            <i>LOSES target status DEFEND</i>
        </effect>
        <group>
            <i>globe</i>
        </group>
    </DEFEND_ACTIVE>
    
    <SACRIFICE_ACTIVE>
        <DESC>Sacrifice Active Component</DESC>
        <effect>
            <i>CHANGE target HP (STAT (CLASS target TEMPLATE) HP - STAT target HP)</i>
            <i>CHANGE target MP (STAT (CLASS target TEMPLATE) MP - STAT target MP)</i>
            <i>LOSES target status KO</i>
        </effect>
        <require>
            <i>'SACRIFICE' not in STAT target status</i>
        </require>
    </SACRIFICE_ACTIVE>
        
    <!-- global moves -->
    <attack>
        <DESC>Attack</DESC>
        <effect>
            <i>CHANGE target HP -((S C MEL + DICE 10) * (1 - S T DEF / 100))</i>
            <i>ACTIVATE target equip weapon</i>
        </effect>
        <group>
            <i>damage</i>
            <i>active</i>
            <i>globe</i>
        </group> 
    </attack>
    
    <pass>
        <DESC>Pass</DESC> 
        <skip>skip</skip>
        <group>
            <i>active</i>
            <i>globe</i>
        </group>
    </pass>
    <!-- fighter moves -->
    <spit>
        <DESC>Toxic Spit</DESC>
        <effect>
            <i>CHANGE target HP -((S C MGK + DICE 10) * (1 - S T MDEF / 100))</i>
