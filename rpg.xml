        </effect>
        <group>
            <i>active</i>
            <i>globe</i>
        </group>
    </defend>
    
    <charge>
        <DESC>Store Energy</DESC>
        <effect>
            <i>GETS caster status CHARGE</i>
            <i>DELAY (CAST target CHARGE_ACTIVE) 2</i>
        </effect>
        <group>
            <i>active</i>
            <i>globe</i>
        </group>
    </charge>
    <!-- bmage moves -->
    <magic>
        <DESC>Magic Missiles</DESC>
        <effect>
            <i>CHANGE target HP -((S C MGK + DICE 10) * (1 - S T MDEF / 100))</i>
