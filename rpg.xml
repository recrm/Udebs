        </effect> 
        <require>
            <i>STAT caster MP  >= 2</i>
        </require>
        <group>
            <i>damage</i>
            <i>active</i>
            <i>globe</i>
        </group>
    </water>
    <!-- wmage moves -->
    
    <sacrifice>
        <DESC>Sacrifice</DESC>
        <skip>skip</skip>
        <effect>
            <i>CHANGE caster HP -(STAT caster HP)</i>
            <i>CHANGE caster MP -(STAT caster MP)</i>
            <i>GETS caster status SACRIFICE</i>
            <i>GETS caster status KO</i>
            <i>CAST (ALL party) SACRIFICE_ACTIVE</i>
        </effect>
        <group>
            <i>active</i>
            <i>globe</i>
        </group>
    </sacrifice>
    
    <heal>
        <DESC>Heal</DESC>
        <effect>
            <i>CHANGE target HP (STAT caster MGK + DICE 10)</i>
            <i>CHANGE caster MP -1</i>
        </effect>
        <require>
            <i>STAT caster MP >= 1</i>
        </require>
        <group>
            <i>health</i>
            <i>active</i>
            <i>globe</i>
        </group>
    </heal>
    
    <antidote>
        <DESC>Antidote</DESC>
        <effect>
            <i>LOSES target status POISON</i>
            <i>CHANGE caster MP -1</i>
        </effect>
        <require>
            <i>STAT caster MP >= 1</i>
        </require>
        <group>
            <i>active</i>
            <i>globe</i>
        </group>
    </antidote>
    
    <!-- nova -->
    
    <doomsday>
        <DESC>Doomsday</DESC>
        <effect>
            <i>DELAY (CAST target DOOM_ACTIVE) 4</i>
            <i>CHANGE caster MP -1</i>
            <i>CHANGE caster ACT -2</i>
        </effect>
        <require>
            <i>STAT caster MP >= 1</i>
        </require>
        <group>
            <i>active</i>
            <i>globe</i>
        </group>
    </doomsday>
    
    <DOOM_ACTIVE>
        <DESC>Doomsday Active Component</DESC>
        <effect>
            <i>CHANGE target HP -(STAT target HP)</i>
        </effect>
        <group>
            <i>damage</i>
            <i>globe</i>
        </group>
    </DOOM_ACTIVE>
    
    <!-- Big Boss -->
    
    <barrier>
        <DESC>Barrier change</DESC>
        <group>
            <i>active</i>
            <i>globe</i>
        </group>
        <effect>
            <i>CHANGE caster MP -1</i>
