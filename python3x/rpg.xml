<udebs>

<name>Sub Battle</name>
<tick>TARGET CHANGE ACT [TARGET STAT SPD * time]</tick>

<definitions>   
    <stats>
        <item>ACT</item> <!-- Number of action points -->
        <item>SPD</item> <!-- How many action points are gained each tick -->
        <item>HP</item>  <!-- Hit points -->
        <item>MP</item>  <!-- Magic points -->
        <item>MEL</item> <!-- Mellee Damage -->
        <item>MGK</item> <!-- Magic Damage -->
        <item>FIRE</item> <!-- Fire weakness -->
        <item>WATER</item> <!-- Water weakness -->
        <item>DEF</item> <!-- physical defense -->
        <item>MDEF</item> <!-- magical defense -->
        <item>LVL</item> <!-- Recruiter Level -->
        <item>r</item>   <!-- Red -->
        <item>g</item>   <!-- Green -->
        <item>b</item>   <!-- Blue -->
    </stats>

    <strings>
        <item>DESC</item> <!-- Printed Descriptions -->
        <item>skip</item>
    </strings>

    <lists>
        <item rlist='rlist' slist='status'>status</item> <!-- Status effects, effects stats -->
        <item rlist='rlist' slist='equip_item'>equip</item> <!-- Equip effects, effects stats -->
        <item>inventory</item> <!-- Place for storing items, party only -->
        <item>monsters</item>  <!-- List of spawnable monsters, recruiter only -->
        <item>yesequip</item>  <!-- List of people that can equip this item. Equipable items only -->
    </lists>
</definitions>

<scripts>
    
    <tick>
        <item>TARGET CHANGE ACT [TARGET STAT SPD * time]</item>
    </tick>

    <init>
        <item>recruiter CAST recruitBattle</item>
        <item>fighter CHANGE ACT [-[fighter STAT ACT] + 1]</item>
        <item>bmage CHANGE ACT [-[bmage STAT ACT] + 1]</item>
        <item>wmage CHANGE ACT [-[wmage STAT ACT]+ 1]</item>
    </init>

    <finish>
        <item>recruiter CHANGE LVL 1</item>
        <item>treasure CHANGE LVL 1</item>
    </finish>
    
    <recruit>
        <item>TARGET CHANGE SPD 1</item>
    </recruit>
    
    <boss>
        <item>boss MOVE travel (1,1)</item>
        <item>boss CHANGE SPD 2</item>
    </boss>
    
</scripts>

<map>
    <row>bmage, empty</row>
    <row>fighter, empty</row> 
    <row>wmage, empty</row>
</map>

