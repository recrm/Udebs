<udebs>

<config>
    <name>Combat</name>
</config>

<definitions>   
    <stats>
        <ACT /> <!-- Number of action points -->
        <SPD /> <!-- How many action points are gained on tick -->
        <HP />  <!-- Hit points -->
        <MP />  <!-- Magic points -->
        <MAXHP />
        <MAXMP />
        <MEL /> <!-- Mellee Damage -->
        <MGK /> <!-- Magic Damage -->
        <FIRE /> <!-- Fire weakness -->
        <WATER /> <!-- Water weakness -->
        <DEF /> <!-- physical defense -->
        <MDEF /> <!-- magical defense -->
        <LVL /> <!-- Recruiter Level -->
        <r />   <!-- Red -->
        <g />   <!-- Green -->
        <b />   <!-- Blue -->
    </stats>

    <strings>
        <DESC /> <!-- Printed Descriptions -->
        <skip />
    </strings>

    <lists>
        <status rlist="" />
        <equip rlist="" />
        <inventory /> <!-- Place for storing items: party -->
        <movelist />
    </lists>
</definitions>

<map>
    <row>fighter, empty</row>
    <row>bmage, empty</row> 
    <row>wmage, empty</row>
</map>

<entities>
    <!-- Script-->
    <init>
        <effect>
            <i>CAST empty recruitBattle</i>
            <i>CHANGE fighter ACT (1 - STAT.fighter.ACT)</i>
            <i>CHANGE bmage ACT (1 - STAT.bmage.ACT)</i>
            <i>CHANGE wmage ACT (1 - STAT.wmage.ACT)</i>
        </effect>
    </init>

    <tick>
        <effect>
            <i>CAST ALL.party atb</i>
            <i>CAST ALL.monster atb</i>
            <i>CAST ALL.party party_activate</i>
            <i>CAST ALL.monster monster_activate</i>
        </effect>
    </tick>
    
    <atb>
        <effect>CHANGE $T ACT STAT.$T.SPD</effect>
    </atb>
    
    <finish>
        <effect>
            <i>CHANGE recruiter LVL 1</i>
            <i>CHANGE treasure LVL 1</i>
        </effect>
    </finish>
    
    <recruitBoss>
        <effect>
            <i>boss TRAVEL (1 1)</i>
            <i>GETS boss group monster</i>
        </effect>
    </recruitBoss>

    <recruitBattle>
        <effect>
            <i>RECRUIT (STAT.recruiter.inventory SUB (DICE.3 + (STAT.recruiter.LVL * 3))) (1 0)</i>
            <i>RECRUIT (STAT.recruiter.inventory SUB (DICE.3 + (STAT.recruiter.LVL * 3))) (1 1)</i>
            <i>RECRUIT (STAT.recruiter.inventory SUB (DICE.3 + (STAT.recruiter.LVL * 3))) (1 2)</i>
            <i>GETS (1 0) group monster</i>
            <i>GETS (1 1) group monster</i>
            <i>GETS (1 2) group monster</i>
        </effect>
    </recruitBattle>
    
    <get_item>
        <effect>
            <i>GETS $C inventory (STAT.treasure.inventory SUB (DICE.5 + (STAT.treasure.LVL * 6)))</i>
            <i>soundFX pickup</i>
        </effect>
    </get_item>
    
    <monster_activate>
        <require>STAT.$T.ACT >= 2</require>
        <effect>
            <i>movelist = STAT.$T.movelist</i>
            <i>enemies = (FILTER ALL.party isnotdead)</i>
            <i>print $enemies</i>
            <i>trandom = ($enemies SUB DICE.2)</i>
            <i>mrandom = ($movelist SUB (DICE (length.$movelist - 1)))</i>
            <i>$T CAST $trandom $mrandom</i>
            <i>message NAME.$T $mrandom $trandom</i>
        </effect>
    </monster_activate>

    <party_activate>
        <require>STAT.$T.ACT >= 2</require>
        <effect>activate $T</effect>
    </party_activate>
    
    <!-- Damage Moves -->
    <globe>
        <DESC>Almost all moves are in this class</DESC>
        <require>
            <i>ko not-in STAT.$caster.status</i>
            <i>ko not-in STAT.$target.status</i>
        </require>
    </globe>
    
    <active>
        <DESC>Actives moves</DESC>
        <require>
            <i>STAT.$caster.MP >= STAT.$move.MP</i>
            <i>STAT.$caster.ACT >= 2</i>
            <i>NAME.$move in STAT.$caster.movelist</i>
        </require>
        <effect>
            <i>CHANGE $caster MP -STAT.$move.MP</i>
            <i>CHANGE $caster ACT -2</i>
        </effect>
    </active>
    
    <status>
        <DESC>status and buff</DESC>
        <require>NAME.$move not-in STAT.$T.status</require>
        <effect>GETS $target status NAME.$move</effect>
    </status>
    
    <damage>
        <DESC>Damaging moves</DESC>
        <effect>DELAY (CAST $target ko) 0</effect>
    </damage>
    
    <!-- Moves -->
    <isnotdead>
        <require>ko not-in STAT.$T.status</require>
    </isnotdead>
    
    <ko>
        <DESC>Characters HP has dropped to 0</DESC>
        <require>STAT.$target.HP &lt;= 0</require>
        <effect>
            <i>CHANGE $target HP -STAT.$target.HP</i>
            <i>LOSES $target group monster</i>
            <i>DELAY (CAST $T party_KO) 0</i>
        </effect>
        <group>
            <i>globe</i>
            <i>status</i>
        </group>
        <SPD>-1</SPD>
    </ko>
    
    <party_KO>
        <require>(length (FILTER ALL.party isnotdead)) == 0</require>
        <effect>
            <i>message You are dead!</i>
            <i>exit</i>
        </effect>
    </party_KO>
    
    <attack>
        <DESC>Attack</DESC>
        <effect>
            <i>soundFX attack</i>
            <i>CHANGE $target HP -(* (STAT.$C.MEL + DICE.10) (1 - (STAT.$T.DEF / 100)))</i>
        </effect>
        <group>
            <i>damage</i>
            <i>active</i>
            <i>globe</i>
        </group>
    </attack>
    
    <pass>
        <DESC>Pass</DESC> 
        <skip>skip</skip>
        <effect>soundFX back</effect>
        <group>
            <i>active</i>
            <i>globe</i>
        </group>
    </pass>
    
    <spit>
        <DESC>Toxic Spit</DESC>
        <effect>
            <i>soundFX magic</i>
            <i>CHANGE $target HP -(* (STAT.$C.MGK + DICE.10) (1 - (STAT.$T.MDEF / 100)))</i>
            <i>CAST $target poison</i>
        </effect>
        <group>
            <i>damage</i>
            <i>active</i>
            <i>globe</i>
        </group>
        <MP>1</MP>
    </spit>
    
    <poison>
        <DESC>DROPS THE CHARACTERS HEALTH ONCE USING A DELAYED CHAIN move</DESC>
        <require>DICE.5 > 2</require>
        <effect>DELAY (CAST $target poison_active) 2</effect>
        <group>
            <i>globe</i>
            <i>status</i>
        </group>
        <r>-30</r>
        <g>-30</g>
        <b>-30</b>
    </poison>
    
    <poison_active>
        <DESC>Toxic Spit Active Part</DESC>
        <effect>
            <i>soundFX hurt</i>
            <i>CHANGE $target HP -(5 + DICE.5)</i>
            <i>DELAY (CAST $target poison_active) 2</i>
        </effect>
        <require>
            <i>poison in STAT.$target.status</i>
        </require>
        <group>
            <i>damage</i>
            <i>globe</i>
        </group>
    </poison_active>
    
    <defend>
        <DESC>Defend</DESC>
        <skip>skip</skip>
        <effect>
            <i>soundFX charge</i>
            <i>DELAY (CAST $caster defend_active) 2</i>
        </effect>
        <require>$C == $T</require>
        <group>
            <i>active</i>
            <i>status</i>
            <i>globe</i>
        </group>
        <DEF>50</DEF>
        <MDEF>30</MDEF>
    </defend>
    
    <defend_active>
        <DESC>Defend Active Component</DESC>
        <effect>LOSES $caster status defend</effect>
        <group>globe</group>
    </defend_active>
    
    <charge>
        <DESC>Store Energy</DESC>
        <effect>
            <i>soundFX charge</i>
            <i>GETS $caster status charge</i>
            <i>DELAY (CAST $target charge_active) 2</i>
        </effect>
        <group>
            <i>active</i>
            <i>globe</i>
        </group>
        <DEF>-50</DEF>
        <MDEF>-30</MDEF>
    </charge>
    
    <charge_active>
        <DESC>Charge Active Component</DESC>
        <effect>
            <i>soundFX attack 2</i>
            <i>CHANGE $target HP -(* 2 (STAT.$C.MEL + DICE.10) (1 - (STAT.$T.DEF / 100)))</i>
            <i>LOSES $caster status CHARGE</i>
        </effect>
        <group>
            <i>damage</i>
            <i>globe</i>
        </group>
    </charge_active>
    
    <magic>
        <DESC>Magic Missiles</DESC>
        <effect>
            <i>soundFX magic</i>
            <i>CHANGE $target HP -(* (STAT.$C.MGK + DICE.10) (1 - (STAT.$T.MDEF / 100)))</i>
        </effect> 
        <group>
            <i>damage</i>
            <i>active</i>
            <i>globe</i>
        </group>
        <MP>1</MP>
    </magic>
    
    <fire>
        <DESC>Fireball</DESC>
        <effect>
            <i>soundFX fire</i>
            <i>CHANGE $target HP -(* (STAT.$C.MGK + DICE.10) (1 - (STAT.$T.MDEF / 100)) STAT.$T.FIRE /2)</i>
        </effect> 
        <group>
            <i>damage</i>
            <i>active</i>
            <i>globe</i>
        </group>
        <MP>2</MP>
    </fire>
    
    <water>
        <DESC>Waterfall</DESC>
        <effect>
            <i>soundFX water</i>
            <i>CHANGE $target HP -(* (STAT.$C.MGK + DICE.10) (1 - (STAT.$T.MDEF / 100)) STAT.$T.WATER /2)</i>
        </effect> 
        <group>
            <i>damage</i>
            <i>active</i>
            <i>globe</i>
        </group>
        <MP>2</MP>
    </water>
    
    <!-- Healing moves groups -->
    <health>
        <DESC>Healing moves</DESC>
        <effect>DELAY (CAST $target overheal) 0</effect>
    </health>
    
    <regen>
        <DESC>Mana Regen Moves</DESC>
        <effect>DELAY (CAST $target overmana) 0</effect>
    </regen>
    
    <!-- Healing moves -->
    
    <overheal>
        <DESC>A check to make sure character hasnt overhealed</DESC>
        <require>
            <i>STAT.$target.HP >= STAT.$target.HP</i>
            <i>party in STAT.$target.group</i>
        </require>
        <effect>CHANGE $target HP (STAT.$T.MAXHP - STAT.$T.HP)</effect>
        <group>globe</group>
    </overheal>
    
    <overmana>
        <DESC>A check to make sure character hasnt overmanad</DESC>
        <require>
            <i>STAT.$target.MP >= STAT.$target.MP</i>
            <i>party in STAT.$T.group</i>
        </require>
        <effect>CHANGE $target MP (STAT.$target.MAXMP - STAT.$target.MP)</effect>
        <group>globe</group>
    </overmana>
    
    <full_heal>
        <DESC>Fully heals target HP and MP</DESC>
        <effect>
            <i>LOSES $T status ko</i>
            <i>CHANGE $T HP (STAT.$target.MAXHP - STAT.$target.HP)</i>
            <i>CHANGE $T MP (STAT.$target.MAXMP - STAT.$target.MP)</i>
        </effect>
        <group>globe</group>
    </full_heal>
    
    <heal>
        <DESC>Heal</DESC>
        <effect>
            <i>soundFX heal</i>
            <i>CHANGE $target HP (STAT.$caster.MGK + DICE.10)</i>
        </effect>
        <group>
            <i>health</i>
            <i>active</i>
            <i>globe</i>
        </group>
        <MP>1</MP>
    </heal>
    
    <antidote>
        <DESC>Antidote</DESC>
        <effect>
            <i>soundFX heal</i>
            <i>LOSES $target status POISON</i>
        </effect>
        <group>
            <i>active</i>
            <i>globe</i>
        </group>
        <MP>1</MP>
    </antidote>
    
    <sacrifice>
        <DESC>Sacrifice</DESC>
        <skip>skip</skip>
        <effect>
            <i>soundFX uberheal</i>
            <i>CAST ALL.party full_heal</i>
            <i>CHANGE $caster HP -STAT.$caster.HP</i>
            <i>CHANGE $caster MP -STAT.$caster.MP</i>
            <i>GETS $caster status ko</i>
        </effect>
        <group>
            <i>status</i>
            <i>active</i>
            <i>globe</i>
        </group>
        <SPD>-1</SPD>
    </sacrifice>
    
    <!-- nova -->
    <doomsday>
        <DESC>Doomsday</DESC>
        <effect>
            <i>soundFX charge</i>
            <i>DELAY (CAST $target doom_active) 4</i>
        </effect>
        <group>
            <i>active</i>
            <i>globe</i>
        </group>
        <MP>1</MP>
    </doomsday>
    
    <doom_active>
        <DESC>Doomsday Active Component</DESC>
        <effect>
            <i>soundFX boom</i>
            <i>CHANGE $target HP -STAT.$target.HP</i>
        </effect>
        <group>
            <i>damage</i>
            <i>globe</i>
        </group>
    </doom_active>
    
    <!-- Big Boss -->
    <current_barrier />
    
    <barrier>
        <DESC>Barrier change</DESC>
        <group>
            <i>active</i>
            <i>globe</i>
        </group>
        <effect>
            <i>soundFX charge</i>
            <i>LOSES $caster status LISTGROUP.boss.status.current_barrier</i>
            <i>CAST $C (STAT.$C.inventory SUB (DICE.5 - 1))</i>
        </effect>
        <MP>1</MP>
    </barrier>
    
    <red>
        <group>
            <i>status</i>
            <i>current_barrier</i>
        </group>
        <MDEF>0</MDEF>
        <DEF>40</DEF>
        <FIRE>0</FIRE>
        <WATER>4</WATER>
        <r>255</r>
        <g>30</g>
        <b>30</b>
    </red>
    
    <blue>
        <group>
            <i>status</i>
            <i>current_barrier</i>
        </group>
        <MDEF>0</MDEF>
        <DEF>40</DEF>
        <FIRE>2</FIRE>
        <WATER>-2</WATER>
        <r>30</r>
        <g>30</g>
        <b>255</b>
    </blue>
    
    <white>
        <group>
            <i>status</i>
            <i>current_barrier</i>
        </group>
        <MDEF>60</MDEF>
        <DEF>-30</DEF>
        <FIRE>-1</FIRE>
        <WATER>-1</WATER>
        <r>255</r>
        <g>255</g>
        <b>255</b>
    </white>
    
    <black>
        <group>
            <i>status</i>
            <i>current_barrier</i>
        </group>
        <r>30</r>
        <g>30</g>
        <b>30</b>
    </black>
    
    <yellow>
        <group>
            <i>status</i>
            <i>current_barrier</i>
        </group>
        <MEL>40</MEL>
        <MGK>40</MGK>
        <MDEF>-40</MDEF>
        <DEF>-40</DEF>
        <FIRE>1</FIRE>
        <WATER>1</WATER>
        <r>255</r>
        <g>255</g>
        <b>30</b>
    </yellow>
        
    <!-- Consumables -->
    <one_use>
        <DESC>ONE use consumable is</DESC>
        <effect>LOSES party inventory $move</effect>
        <require>
            <i>NAME.$move in STAT.party.inventory</i>
            <i>SACRIFICE not-in STAT.$target.status</i>
        </require>
    </one_use>
    
    <potion>
        <DESC>An HP potion</DESC>
        <group>
            <i>one_use</i>
            <i>health</i>
        </group>
        <require>STAT.$T.HP != STAT.$T.MAXHP</require>
        <effect>
            <i>CHANGE $target HP 50</i>
            <i>LOSES $target status ko</i>
            <i>LOSES $target status POISON</i>
        </effect>
    </potion>
    
    <mana>
        <DESC>An MP Potion</DESC>
        <group>
            <i>one_use</i>
            <i>regen</i>
        </group>
        <require>STAT.$T.MP != STAT.$T.MAXMP</require>
        <effect>
            <i>CHANGE $target MP 10</i>
            <i>LOSES $target status POISON</i>
        </effect>
    </mana>
    
    <!-- equipable -->
    <equipable />
    
    <equip_item>
        <DESC>equipable is</DESC>
        <require>
            <i>NAME.$move in STAT.party.inventory</i>
            <i>NAME.$T in (STAT $move inventory)</i>
        </require>
        <effect>
            <i>$T CAST (LISTGROUP $T equip CLASS.$move.equipable) unequip</i>
            <i>LOSES party inventory $move</i>
            <i>GETS $T equip $move</i>
        </effect>
    </equip_item>
    
    <unequip>
        <effect>
            <i>LOSES $C equip $T</i>
            <i>GETS party inventory $T</i>
        </effect>
    </unequip>
    
    <unequip_all>
        <effect>$T CAST STAT.$T.equip unequip</effect>
    </unequip_all>
    
    <weapon>
        <DESC>General weapon Class</DESC>
    </weapon>
    
    <sword>
        <DESC>sword class for is</DESC>
        <MEL>1</MEL>
        <inventory>fighter</inventory>
        <group>equipable</group>
    </sword>
    
    <staff>
        <DESC>staff class for is</DESC>
        <inventory>
            <i>wmage</i>
            <i>bmage</i>
        </inventory>
        <group>equipable</group>
    </staff>

    <hat>
        <DESC>hat class for is.</DESC>
        <MAXHP>10</MAXHP>
        <inventory>
            <i>fighter</i>
            <i>wmage</i>
            <i>bmage</i>
        </inventory>
        <group>equipable</group>
    </hat>
    
    <!-- hats!! -->
    <bor_hat>
        <DESC>A boring hat</DESC>
        <MAXHP>10</MAXHP>
        <DEF>10</DEF>
        <MDEF>10</MDEF>
        <group>
            <i>hat</i>
            <i>equip_item</i>
        </group>
    </bor_hat>
    
    <med_hat>
        <DESC>A mediocre hat</DESC>
        <MAXHP>20</MAXHP>
        <DEF>20</DEF>
        <MDEF>20</MDEF>
        <group>
            <i>hat</i>
            <i>equip_item</i>
        </group>
    </med_hat>
    
    <metal_hat>
        <DESC>A metal hat</DESC>
        <MAXHP>30</MAXHP>
        <DEF>30</DEF>
        <MDEF>30</MDEF>
        <group>
            <i>hat</i>
            <i>equip_item</i>
        </group>
    </metal_hat>
    
    <fancy_hat>
        <DESC>A fancy hat</DESC>
        <MAXHP>20</MAXHP>
        <DEF>20</DEF>
        <MDEF>20</MDEF>
        <MGK>20</MGK>
        <group>
            <i>hat</i>
            <i>equip_item</i>
        </group>
    </fancy_hat>
    
    <!-- swords -->
    <wood_sword>
        <DESC>A wooden Sword</DESC>
        <group>
            <i>sword</i>
            <i>equip_item</i>
            <i>weapon</i>
        </group>
        <MEL>10</MEL>
    </wood_sword>
    
    <metal_sword>
        <DESC>A metal Sword</DESC>
        <group>
            <i>sword</i>
            <i>equip_item</i>
            <i>weapon</i>
        </group>
        <MEL>20</MEL>
    </metal_sword>
    
    <shiny_sword>
        <DESC>A shiny Sword</DESC>
        <group>
            <i>sword</i>
            <i>equip_item</i>
            <i>weapon</i>
        </group>
        <MEL>30</MEL>
    </shiny_sword>
    
    <!-- staffs -->
    <wood_staff>
        <DESC>A wooden Staff</DESC>
        <group>
            <i>staff</i>
            <i>equip_item</i>
            <i>weapon</i>
        </group>
        <MGK>10</MGK>
        <MAXMP>5</MAXMP>
    </wood_staff>
    
    <metal_staff>
        <DESC>A metal Staff</DESC>
        <group>
            <i>staff</i>
            <i>equip_item</i>
            <i>weapon</i>
        </group>
        <MGK>20</MGK>
        <MAXMP>10</MAXMP>
    </metal_staff>
    
    <shiny_staff>
        <DESC>A shiny staff</DESC>
        <group>
            <i>staff</i>
            <i>equip_item</i>
            <i>weapon</i>
        </group>
        <MGK>30</MGK>
        <MAXMP>15</MAXMP>
    </shiny_staff>
        
    <!-- Meta Units -->
    <recruiter>
        <DESC>The board recruiter</DESC>
        <inventory>
            <i>zombie</i>
            <i>ghost</i>
            <i>zombie</i>
            <i>druid</i>
            <i>golem</i>
            <i>golem</i>
            <i>fire_golem</i>
            <i>wisp</i>
            <i>Wisp</i>
            <i>nova</i>
        </inventory>
    </recruiter>
    
    <treasure>
        <DESC>Unit that hands out treasure</DESC>
        <inventory>
            <i>bor_hat</i>
            <i>wood_sword</i>
            <i>wood_staff</i>
            <i>potion</i>
            <i>mana</i>
            <i>potion</i>
            <i>med_hat</i>
            <i>metal_staff</i>
            <i>metal_sword</i>
            <i>potion</i>
            <i>mana</i>
            <i>mana</i>
            <i>metal_hat</i>
            <i>fancy_hat</i>
            <i>shiny_sword</i>
            <i>shiny_staff</i>
            <i>potion</i>
            <i>mana</i>
        </inventory>
    </treasure>
    
    <meta />
    
    <party>
        <DESC>heroes main class</DESC>
        <movelist>
            <i>attack</i>
            <i>pass</i>
        </movelist>
        <inventory>
            <i>potion</i>
            <i>mana</i>
        </inventory>
        <group>meta</group>
        <HP>0</HP>
    </party>
    
    <!-- Fighter -->
    <fighter>
        <DESC>Fighter the Brave</DESC>
        <MAXHP>130</MAXHP>
        <MAXMP>1</MAXMP> 
        <HP>130</HP>
        <MP>1</MP>
        <MEL>45</MEL>
        <MGK>10</MGK>
        <DEF>20</DEF>
        <FIRE>2</FIRE>
        <WATER>2</WATER>
        <SPD>1</SPD>
        <r>255</r>
        <g>75</g>
        <b>75</b>  
        <movelist>
            <i>spit</i>
            <i>defend</i>
            <i>charge</i>
        </movelist>
        <group>party</group>
    </fighter>
    
